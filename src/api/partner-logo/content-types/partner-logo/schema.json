{
  "kind": "collectionType",
  "collectionName": "partner_logos",
  "info": {
    "singularName": "partner-logo",
    "pluralName": "partner-logos",
    "displayName": "Partner Logo"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": ["corporate", "media", "ngo", "government", "other"],
      "required": true,
      "default": "corporate"
    },
    "logoAsset": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"],
      "required": true
    }
  }
}
